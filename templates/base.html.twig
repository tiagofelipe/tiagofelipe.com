<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    {% block metas %}
        <meta property="robots" content="index,follow"/>
        <meta property="googlebot" content="index, follow"/>
        <meta property="revisit-after" content="5 days"/>
        <meta property="language" content="pt-br"/>
        <meta property="rating" content="general"/>
        <meta property="page-topic"
              content="{{ metaPageTopic is defined ? metaPageTopic : 'Wtis Soluções em Tecnologia e Marketing para Web' }}"/>
        <meta property="classification"
              content="{{ metaPageClassification is defined ? metaPageClassification : 'Wtis Soluções em Tecnologia e Marketing para Web' }}"/>
        <meta property="distribution" content="Global"/>
        <meta property="author" content="Wtis"/>
        <meta property="copyright" content="wtis"/>
        <meta property="og:type" content="website">
    {% endblock %}
    {% block title %}
        <title>Marketing Digital e Fábrica de Software - Wtis</title>
    {% endblock %}
    {% block stylesheets %}
        {% if app.environment == 'dev' %}
            <link href="{{ asset('css/jquery-ui.css') }}" rel="stylesheet">
            <link href="{{ asset('css/bootstrap.min.css') }}" rel="stylesheet">
            <link href="{{ asset('css/tipTip.css') }}" rel="stylesheet">
            <link href="{{ asset('css/style.css') }}" rel="stylesheet">
        {% else %}
            <link href="{{ asset('css/app.min.css') }}" rel="stylesheet">
        {% endif %}
        <link href='https://fonts.googleapis.com/css?family=Open+Sans:600,300,400' rel='stylesheet' type='text/css'>
    {% endblock %}
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>
<!-- HEADER -->
{% include "header.html.twig" %}
<!--/ HEADER -->
<!-- CONTENT -->
<div class="site">
    {% block body %}{% endblock %}
</div>
<!--//// CONTENT -->
<!-- FOOTER -->
{% include "footer.html.twig" %}
<!--//// FOOTER -->

{% block javascripts %}
    {% if app.environment == 'dev' %}
    <script src="{{ asset('js/vendor/jquery.min.js') }}"></script>
    <script src="{{ asset('js/vendor/jquery-ui.min.js') }}"></script>
    <script src="{{ asset('js/vendor/bootstrap.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/vendor/jquery.localscroll-1.2.7-min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/vendor/jquery.scrollTo-1.4.2-min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/vendor/jquery.tipTip.minified.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/vendor/scrollReveal.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/vendor/jquery.mask.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/vendor/parsley.min.js') }}"></script>
    <script src="{{ asset('js/site.js') }}"></script>
    {% else %}
    <script src="{{ asset('js/app.min.js') }}"></script>
    {% endif %}
{% endblock %}
</body>
</html>
